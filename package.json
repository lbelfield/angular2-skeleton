{
  "name": "luke",
  "version": "1.0.0",
  "description": "awesome ts app",
  "main": "index.js",
  "scripts": {
    "tslintInitial": "tslint --init",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tslint": "tslint -c tslint.json app/greeter.ts",
    "rimraf": "rimraf js build dist",
    "babel": "babel ./js --presets es2015 --out-dir ./build",
    "bundle": "webpack",
    "preminify": "uglifyjs ./dist/vendor-bundle.js -c -m -o ./dist/vendor-min.js ",
    "minify": "uglifyjs ./dist/main-bundle.js -c -m -o ./dist/main-min.js ",
    "prebuild": "npm run tslint && npm run rimraf",
    "build": "tsc && npm run babel",
    "postbuild": "npm run bundle && npm run minify",
    "start": "http-server ./ -a localhost -p 3000 -c-1"
  },
  "author": "luke",
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "^2.4.1",

    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.6",
    "style-loader": "^0.13.1",
    "tslint": "^4.0.1",
    "typescript": "^2.0.10",
    "typings": "^2.0.0",
    "uglifyjs": "^2.4.10",
    "url-loader": "^0.5.7",
